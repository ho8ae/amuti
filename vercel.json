{
    "rewrites": [
      {
        "source": "/(.*)",
        "destination": "https://yejinsports.com/$1",
        "has": [
          {
            "type": "host",
            "value": "yejinsports.co.kr"
          }
        ]
      },
      {
        "source": "/(.*)",
        "destination": "https://yejinsports.com/$1", 
        "has": [
          {
            "type": "host",
            "value": "www.yejinsports.co.kr"
          }
        ]
      }
    ]
}